<mapper namespace="cn.edu.guet.mapper.EmpSalaryMapper">
    <select id="findSalary" parameterType="String" resultType="cn.edu.guet.bean.EmpSalary">
        SELECT * FROM EMP_SALARY_TABLE where EMP_ID=#{id}
